   "source": [
    "# Map Loading Performance Diagnosis\n",
    "\n",
    "Analyzing why the map takes too long to load and raster API is failing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import os\n",
    "import time\n",
    "import requests\n",
    "from pathlib import Path\n",
    "\n",
    "# Check file sizes and loading bottlenecks\n",
    "data_dir = Path('/home/tin_2/ssd-disk/TreeHack2026_YC/public/data')\n",
    "\n",
    "print(\"=== File Size Analysis ===\")\n",
    "for file in data_dir.glob('*'):\n",
    "    size_mb = file.stat().st_size / (1024 * 1024)\n",
    "    print(f\"{file.name}: {size_mb:.2f} MB\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Test TIFF file accessibility and format\n",
    "tiff_path = data_dir / 'california_irradiation_2025.tif'\n",
    "\n",
    "print(\"=== TIFF File Analysis ===\")\n",
    "print(f\"File exists: {tiff_path.exists()}\")\n",
    "print(f\"File readable: {os.access(tiff_path, os.R_OK)}\")\n",
    "\n",
    "if tiff_path.exists():\n",
    "    try:\n",
    "        from osgeo import gdal\n",
    "        dataset = gdal.Open(str(tiff_path))\n",
    "        if dataset:\n",
    "            print(f\"Projection: {dataset.GetProjection()}\")\n",
    "            print(f\"Size: {dataset.RasterXSize} x {dataset.RasterYSize}\")\n",
    "            print(f\"Bands: {dataset.RasterCount}\")\n",
    "            \n",
    "            # Check if it's EPSG:3857\n",
    "            srs = dataset.GetSpatialRef()\n",
    "            if srs:\n",
    "                print(f\"EPSG Code: {srs.GetAuthorityCode(None)}\")\n",
    "        else:\n",
    "            print(\"Failed to open TIFF with GDAL\")\n",
    "    except ImportError:\n",
    "        print(\"GDAL not available - this is likely the raster API issue\")\n",
    "    except Exception as e:\n",
    "        print(f\"GDAL error: {e}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Test GeoJSON loading performance\n",
    "import json\n",
    "\n",
    "print(\"=== GeoJSON Loading Performance ===\")\n",
    "\n",
    "geojson_files = [\n",
    "    'BuildingClimateZones_CEC_2015_-254197201195074155.geojson',\n",
    "    'MDHD_Dashboard_ArcGIS_Updated_Nov_6901160749949423404.geojson'\n",
    "]\n",
    "\n",
    "for filename in geojson_files:\n",
    "    filepath = data_dir / filename\n",
    "    if filepath.exists():\n",
    "        start_time = time.time()\n",
    "        with open(filepath, 'r') as f:\n",
    "            data = json.load(f)\n",
    "        load_time = time.time() - start_time\n",
    "        \n",
    "        feature_count = len(data.get('features', []))\n",
    "        print(f\"{filename}:\")\n",
    "        print(f\"  Load time: {load_time:.2f}s\")\n",
    "        print(f\"  Features: {feature_count}\")\n",
    "        print(f\"  Size: {filepath.stat().st_size / (1024*1024):.2f} MB\")\n",
    "        print()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Identify bottlenecks\n",
    "print(\"=== Performance Bottleneck Analysis ===\")\n",
    "print(\"1. Large GeoJSON files loading synchronously\")\n",
    "print(\"2. GDAL dependency missing for raster processing\")\n",
    "print(\"3. No lazy loading or progressive enhancement\")\n",
    "print(\"4. Multiple large files loaded on map initialization\")\n",
    "\n",
    "print(\"\\n=== Recommended Solutions ===\")\n",
    "print(\"1. Implement lazy loading for layers\")\n",
    "print(\"2. Pre-process TIFF to tiles or use simpler raster approach\")\n",
    "print(\"3. Add loading states and progressive enhancement\")\n",
    "print(\"4. Cache processed data\")"
   ]
  }
